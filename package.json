{
  "name": "React-Boil",
  "version": "1.0.0",
  "description": "Cutting edge PWA React.js boilerplate.",
  "main": "hot.js",
  "scripts": {
    "dll": "webpack --config webpack.dll.js ",
    "clear": "rm -rf dll/ dist/",
    "clean": "rm -rf dist/",
    "dev": "node hot.js",
    "start": "yarn run clear && yarn run dll && yarn run ",
    "client": "webpack --config webpack.client.js --env.production",
    "server": "webpack --config webpack.server.js --env.production",
    "prod": "webpack --config webpack.config.js --env.production",
    "preview": "yarn run clear && yarn run prod && cd dist/ && node server.js",
    "analyzer": "webpack --config webpack.client.js --env.production --env.analyzer",
    "test": "jest --coverage",
    "lint": "tslint --type-check --project tsconfig.json",
    "docker/build": "docker build -f internal/docker/Dockerfile -t cescoferraro/boil:latest .",
    "docker/run": "docker run -p 4000:4000 -v dist/:/srv/boil cescoferraro/boil:latest",
    "docker/push": "docker push cescoferraro/boil:latest",
    "docker/preview": "yarn run clean && yarn build && yarn docker/run",
    "push": "yarn run clean && yarn run prod && yarn run docker/build && yarn run docker/push",
    "webpack-defaults": "webpack-defaults"
  },
  "author": "Francesco Antonello Ferraro",
  "license": "ISC",
  "dependencies": {
    "@types/debug": "^0.0.29",
    "@types/jest": "^20.0.5",
    "@types/material-ui": "^0.17.18",
    "@types/offline-js": "^0.7.28",
    "@types/react": "^15.0.35",
    "@types/react-redux": "^4.4.45",
    "awesome-typescript-loader": "^3.2.2",
    "cachedfs": "^0.3.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.4",
    "eslint-config-cesco": "^0.0.2",
    "express": "^4.15.3",
    "extract-css-chunks-webpack-plugin": "1.0.15",
    "extract-text-webpack-plugin": "^2.1.2",
    "faker": "^4.1.0",
    "favicons-webpack-plugin-cesco": "^0.0.6",
    "file-loader": "^0.11.2",
    "global": "^4.3.2",
    "history": "^4.6.3",
    "html-webpack-plugin": "^2.29.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.3.1",
    "jest": "^20.0.4",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.7",
    "morgan": "^1.8.2",
    "noop-webpack-plugin": "^1.0.1",
    "offline-js": "^0.7.19",
    "org": "^0.2.0",
    "org-loader": "^1.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "rapscallion": "^2.1.13",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-flag-icon-css": "^1.0.19",
    "react-flags": "wiredmax/react-flags#9/head",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "^5.0.5",
    "react-redux-firebase": "1.5.0-beta.3",
    "react-redux-toastr": "^7.1.0",
    "react-sizeme": "^2.3.4",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "react-universal-component": "^2.0.19",
    "react-world-flags": "^0.0.3",
    "recompose": "^0.24.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.9.7",
    "redux-first-router-link": "^1.1.3",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.14.1",
    "redux-storage": "^4.1.2",
    "redux-storage-decorator-debounce": "^1.1.3",
    "redux-storage-engine-localstorage": "^1.1.4",
    "rxjs": "^5.4.2",
    "serialize-javascript": "^1.4.0",
    "source-map-loader": "^0.2.1",
    "stats-webpack-plugin": "^0.6.1",
    "svg-react-loader": "^0.4.4",
    "svg-to-react": "^0.1.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-jest": "^20.0.7",
    "tslint": "^5.5.0",
    "tslint-react": "^3.1.0",
    "typed-css-modules-loader": "^0.0.10",
    "typescript": "^2.4.2",
    "uglifyjs-webpack-plugin": "v1.0.0-beta.1",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-flush-chunks": "^1.1.14",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.1.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "eslint": "^4.3.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
